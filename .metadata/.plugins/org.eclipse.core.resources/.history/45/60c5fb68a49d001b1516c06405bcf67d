
public class TestaWhile {


	    public static void main(String[] args) {
	        int contador = 0;
	        while(contador <= 10) {
	            System.out.println(contador);
	           // contador = contador + 1;  Queremos somar 1 nele mesmo
	            contador += 1;
	        }
	        System.out.println(contador);

	    }
	}

