import java.util.Scanner;

public class Diferença {
	public static void main(String [] args) {
		Scanner sc = new Scanner (System.in);
		
		
		int A,B,C,D,diferença;
		diferença = A * B - C * D;
		
		System.out.printf("Digite o valor de A\n");
		A = sc.nextInt();
		
		System.out.printf("Digite o valor de A\n");
		B = sc.nextInt();
		
		System.out.printf("Digite o valor de A\n");
		C = sc.nextInt();
		
		System.out.printf("Digite o valor de A\n");
		D = sc.nextInt();
		
		System.out.printf("A diferença entre os valores A, B, C e D é igual a", diferença);
		
		sc.close();
		
	}

}
